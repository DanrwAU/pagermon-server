{
  "name": "Gotify",
  "description": "Push notifications with Gotify",
  "disable": false,
  "trigger": "message",
  "scope": "after",
  "config": [
    {
      "name": "URL",
      "label": "Gotify URL",
      "type": "text",
      "required": true

    },
    {
      "name": "port",
      "label": "Gotify Port",
      "type": "text"
    },
    {
      "name": "APIKey",
      "label": "API Key",
      "description": "APP API key",
      "type": "text",
      "required": true
    }
  ],
  "aliasConfig": [
    {
      "name": "enable",
      "label": "Enable",
      "description": "Enable sending messages that match this alias via Gotify",
      "type": "checkbox"
    },
    {
      "name": "priority",
      "label": "Priority",
      "description": "Priority of the message",
      "type": "number"
    }
  ]
}
